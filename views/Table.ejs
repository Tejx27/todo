<style>
    #itemlist{
        font-family:monospace;
        
    }
</style>
<h2 id="itemlist" class="text-center">Items</h2>
<table class="table table-striped">
    <tr>
        <th>TODOS</th>
        <th>Time1</th>
        <th>Time2</th>
    </tr>
    <% for(var i=0; i < items.length; i++) { %>
        <tr>
            <td><%= items[i].name %></td>
            <td><%= items[i].time %></td>
            <td><%= items[i].time2 %></td>
        </tr>
    <% } %>
</table>

<h2 id="itemlist" class="text-center">Lists</h2>
<table class="table table-dark table-striped">
    <tr>
        <th>Name</th>
    </tr>
    <% for(var i=0; i < lists.length; i++) { %>
        <tr>
            <td><%= lists[i].name %></td>
        </tr>
    <% } %>
</table>
